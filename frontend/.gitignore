lib/**/*.gr.dart

# Generated model files
lib/**/*.g.dart
lib/**/*.freezed.dart

# ==================== Assets ====================
# Temporary asset files
assets/temp/
assets/*.tmp

# ==================== Documentation ====================
# Documentation build files
doc/api/

# ==================== IDE specific ====================
# Android Studio
.gradle/
local.properties

# Xcode
*.xcworkspace
!default.xcworkspace
*.xcuserstate
xcuserdata/

# ==================== CI/CD ====================
# GitHub Actions secrets
.github/workflows/secrets/

# ==================== Backup files ====================
*.bak
*.backup
*.orig

# ==================== Log files ====================
*.log
logs/
flutter_*.log

# ==================== Temporary directories ====================
tmp/
temp/
