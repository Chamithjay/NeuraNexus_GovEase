# Firebase configuration (add these if using Firebase)
# android/app/google-services.json
# ios/Runner/GoogleService-Info.plist
# ios/firebase_app_id_file.json
.firebase/
firebase-debug.log
.firebaserc
firebase.json

# ==================== Environment & Secrets ====================
.env
.env.local
.env.*.local
lib/config/api_keys.dart
lib/config/secrets.dart

# ==================== Build outputs ====================
# APK files
*.apk
*.aab

# IPA files
*.ipa

# App bundles
*.app

# ==================== Debugging ====================
# Flutter debugging files
.flutter-plugins-dependencies.bak

# ==================== Dependencies ====================
# pubspec.lock variations
pubspec.lock.bak

# ==================== Platform runners ====================
